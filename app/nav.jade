nav.navbar.navbar-default.navbar-fixed-top(role='navigation', bs-navbar)
  .container-fluid
      .navbar-header
        a.navbar-brand(href='#') Chesby
        button.navbar-toggle(type='button', ng-init="isCollapsed = true", ng-click="dd1 = false; isCollapsed = !isCollapsed")
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
          span.icon-bar

      .collapse.navbar-collapse(ng-class="{in: !(isCollapsed && !dd1)}", ng-click="isCollapsed = true")
        ul.nav.navbar-nav
          //li(data-match-route='^/$')
            a(href='#/') Home
          //li(data-match-route='/faq')
            a(href='#/faq') FAQ   
          li(data-match-route='/transloader')
            a(href="#/transloader")
              span.glyphicon.glyphicon-paperclip
              |  Transloader 
          li(data-match-route='/bookings')
            a(href="#/bookings")
              span.glyphicon.glyphicon-calendar
              |  Bookings
          li
            a(href="")
              span.glyphicon.glyphicon-shopping-cart
              |  Cart
          li(data-match-route='/search')
            a(href="#/search")
              span.glyphicon.glyphicon-search
              |  Search
          li(data-match-route='/invoices')
            a(href="#/invoices")
              span.glyphicon.glyphicon-pencil
              |  Invoices 
          li(data-match-route='/admin')
            a(href="#/admin")
              //span.glyphicon.glyphicon-edit
              span.glyphicon.glyphicon-user
              |  Admin 
          //li
            a(href="/videobg")
              span.glyphicon.glyphicon-picture
              |  VideoBG 

          li(data-match-route='/messages')
            a(href="#/messages")
              span.glyphicon.glyphicon-envelope
              |  Messages 
              span.label.label-danger {{unreadMessages}}
              

          //ul.nav.navbar-nav(ng-if="account.roleValue >= 20")
            li(data-match-route='/page-two.*')
            a(href='#/page-two/sub-a') Page Two

        ul.nav.navbar-nav.navbar-right(ng-hide="user.auth")
          li: button.btn.btn-default.navbar-btn(ng-click="auth.popup()") Log in / sign up

          //li(data-match-route='/users.*')
            a(ng-href='#/users/{{user.uid}}') Profile 
          
            
        ul.nav.navbar-nav.navbar-right#ifauthed(ng-show="user.auth")
          li.dropdown(ng-class="{ open : dd1 }", ng-init="dd1 = false", ng-click="dd1 = !dd1")
            a.dropdown-toggle(href="") Logged in as {{user.name || user.displayName || user.uid}}
              span.caret
            ul.dropdown-menu
              li
                a(ng-click="auth.unauth()") Logout
              li.divider
              li(data-match-route='/profile')
                a(href="#/profile") Profile
              li(data-match-route='/files')
                a(href='#/files') Files
 
            
            


    
    
      
      